{
  "name": "node-http-server",
  "version": "1.0.0",
  "description": "Node.js example server starter project",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run dev:client && npm run dev:server",
    "dev:server": "nodemon --exec \"babel-node server/index.js\"",
    "dev:client": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "cookie-session": "1.4.0",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "concurrently": "6.2.1",
    "nodemon": "1.18.4"
  },
  "keywords": [],
  "nodemonConfig": {
    "ignore": ["test/*", "docs/*", "client/build/*"],
    "delay": 2500
  }
}